{
    "version": 2,
    "builds": [
      {
        "src": "be-DADN-watering-system/src/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "be-DADN-watering-system/src/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }